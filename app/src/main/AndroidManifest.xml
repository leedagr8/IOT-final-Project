        </activity>
        <activity android:name=".NewUser" />
        <activity android:name=".power_utility" />
        <activity android:name=".ApplianceList" />
        <activity android:name=".utility" />
        <activity android:name=".Switch" />
        <activity android:name=".HomeLogin" />
        <activity android:name=".home_main" />
        <activity android:name=".PowerReg" />
        <activity android:name=".UtilReg" />
        <activity android:name=".powerlogin" />
        <activity android:name=".utillogin" />
        <activity android:name=".alarmSystem" />
    </application>

</manifest>